<%= turbo_frame_tag cat do %>
  <%= bootstrap_form_with(model: cat) do |form| %>

    <%= form.text_field :name, required: false %>
    <%= form.number_field :age, required: false %>

    <%= form.primary %>
    <%= link_to "キャンセル", cats_path, class: "btn btn-sm btn-outline-secondary" %>
  <% end %>
<% end %>
